{
  "name": "edeoliv/gpt-translate",
  "description": "A Laravel package to create a base translation file, and translate it to any language with ChatGPT API",
  "type": "laravel-package", 
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Edeoliv\\GptTranslate\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Elias Olivtradet",
      "email": "deoliveira.elias.eus@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "require": {
    "php": "^8.3",
    "openai-php/laravel": "^0.10.1"
  }
}
